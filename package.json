    "hot-hook": "^0.4.0",
    "pino-pretty": "^13.0.0",
    "prettier": "^3.4.2",
    "ts-node-maintained": "^10.9.4",
    "typescript": "~5.7"
  },
  "dependencies": {
    "@adonisjs/auth": "^9.2.4",
    "@adonisjs/core": "^6.17.0",
    "@adonisjs/cors": "^2.2.1",
    "@adonisjs/lucid": "^21.6.0",
    "@adonisjs/redis": "^9.2.0",
